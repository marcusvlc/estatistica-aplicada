figura_notas<-hist(notas,breaks=seq(3.5,10,by=1.3),right=FALSE,plot=TRUE)
aux<-with(figura_notas,100*density*diff(breaks)[1])
labs<-paste(round(aux),"%", sep="")
plot(figura_notas,freq = FALSE, labels = labs, ylab="Densidade de Frequ?ncia", xlab="Notas", col="green",border="black", main="Histograma para a vari?vel Notas",xlim=c(3.5,10), xaxp=c(3.5,10,5),ylim=c(0,.2),cex.main=2,cex.lab=1.5,cex.axis=1.5)
